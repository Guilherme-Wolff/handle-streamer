    "proxy-agent": "^6.4.0",
    "reflect-metadata": "^0.1.13",
    "terser": "^5.39.0",
    "tiktok-live-connector": "1.2.0",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.26",
