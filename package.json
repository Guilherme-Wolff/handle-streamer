{
  "name": "central-system-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "licenses": [
    "MIT"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "start_dev": "node --loader ts-node/esm index.ts",
    "start_dev_inspect": "cd dist && pm2 start index.js --watch",
    "start_builded": "cd dist && node index.js",
    "build": "npx tsc && node tarser-process.js && copyfiles -u 1 src/**/*.json dist/ && copyfiles .env dist/",
    "builded_run": "cd dist && node index.js",
    "start": "env HOST=localhost NODE_ENV=start node dist/index.js",
    "build_run": "npx tsc && copyfiles -u 1 src/**/*.json dist/ && copyfiles .env dist/ && cd dist && node index.js",
    "pm2": "pm2 start dist/index.js",
    "dev": "nodemon --exec node --loader ts-node/esm index.ts",
    "prestart": "npm run build",
    "docker": "npm run build && node dist/index.js",
    "migration:generate": "typeorm-ts-node-esm migration:generate ./src/api/infrastructure/migrations  -d ./src/api/application/config/datasource.ts",
    "migration:run": "typeorm-ts-node-esm migration:run -d ./src/api/application/config/datasource.ts",
    "migration:create": "typeorm migration:create ./src/api/infrastructure/migrations/%MIGRATON_NAME%",
    "generate-files": "node scripts/generate.js",
    "test": "set NODE_ENV=test & jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.5.0",
    "@swc/core": "^1.9.1",
    "@swc/helpers": "^0.5.13",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "@types/pg": "^8.10.2",
    "@types/uuid": "^9.0.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.20",
    "prettier": "^3.0.3",
    "sqlite3": "^5.1.6",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-sinon": "^2.0.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@types/estraverse": "^5.1.7",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "chai": "^4.3.8",
    "cheerio": "^1.0.0",
    "chokidar": "^4.0.3",
    "class-validator": "^0.14.0",
    "cluster": "^0.7.7",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "esprima": "^4.0.1",
    "estraverse": "^5.3.0",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
